<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <script src="/javascripts/vue.js"></script>
    <title>hello world</title>
</head>
<body>
    <div id="app">
        {{message}}
        <div class="dongtai" v-bind:title="timemessage">
            {{timemessage}}
        </div>
    </div>
</body>
<script>
    var app = new Vue(
        {
            el: "#app",
            data: {
                message:"hello jesse",
                timemessage: "现在是"+new Date().toLocaleTimeString()
            }
        }
    );
</script>
</html>